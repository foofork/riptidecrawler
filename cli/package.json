{
  "name": "@riptide/cli",
  "version": "1.1.0",
  "description": "Command-line interface for RipTide web crawler API",
  "type": "module",
  "bin": {
    "riptide": "./bin/riptide.js"
  },
  "keywords": [
    "cli",
    "web-scraping",
    "crawler",
    "riptide",
    "content-extraction",
    "web-crawler",
    "scraper"
  ],
  "author": "RipTide Team <team@riptide.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/riptide-api.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/your-org/riptide-api/issues"
  },
  "homepage": "https://github.com/your-org/riptide-api/tree/main/cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src bin",
    "format": "prettier --write \"src/**/*.js\" \"bin/**/*.js\"",
    "prepublishOnly": "npm run lint && npm test"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "axios": "^1.6.2",
    "conf": "^12.0.0",
    "cli-table3": "^0.6.3",
    "boxen": "^7.1.1",
    "update-notifier": "^7.0.0",
    "nanospinner": "^1.1.0",
    "picocolors": "^1.0.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0"
  }
}
